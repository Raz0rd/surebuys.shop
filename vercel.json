{
  "version": 2,
  "name": "priv8pages-generator",
  "alias": ["gmeports.com.br", "www.gmeports.com.br"],
  "builds": [
    {
      "src": "whitepriv8/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/priv8pages/(.*)",
      "dest": "/whitepriv8/$1"
    },
    {
      "src": "/priv8pages",
      "dest": "/whitepriv8/"
    },
    {
      "src": "/(.*)",
      "dest": "/whitepriv8/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "whitepriv8/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
